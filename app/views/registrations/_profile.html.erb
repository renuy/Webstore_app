    <%= user.username %><br/>
    <%= user.email %><br/>
    <% user.member.each do |member| %>
     <br/>
     <%= "Justbooks ID: #{member.valid_card[0].card_id}" %> 
     
      <% if member.valid_card[0].renewable? %>
      <%= link_to 'renew', "#", :id=>"#{member.valid_card[0].card_id}" ,:class=>'suggest'%>
        <div class = "dialog" id="<%="dialog_#{member.valid_card[0].card_id}" %>">
          Renew for: 
            <% member.valid_card[0].plan.renewMonthsArr.each do |month| %>
              <%=month%>-<%= radio_button_tag("months", month.to_s, false) %>
            <% end %> 
          months
        </div>
              <br/>
      <% end %>
    <% end %>
